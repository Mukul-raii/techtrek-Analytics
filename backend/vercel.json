{
  "version": 2,
  "env": {
    "ENABLE_SQL": "false",
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**/*.js"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ]
}
